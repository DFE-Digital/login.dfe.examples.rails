<!DOCTYPE html>
<html>
  <head>
    <title>LoginDfeExamplesRails</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if !@current_user.nil? %>
      <h1>Welcome, <%= @current_user['given_name'] -%> <%= @current_user['family_name'] -%> (<%=@current_user['email']-%>)</h1>
      <br />
    <% end %>
    <%= yield %>
  </body>
</html>
